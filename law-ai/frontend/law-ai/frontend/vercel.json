{
  "version": 2,
  "name": "lawai-saas",
  "alias": ["lawai.ragspro.com"],
  "build": {
    "env": {
      "NEXT_PUBLIC_SITE_URL": "https://lawai.ragspro.com"
    }
  },
  "env": {
    "NEXT_PUBLIC_SITE_URL": "https://lawai.ragspro.com"
  },
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}